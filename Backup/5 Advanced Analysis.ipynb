   "source": [
    "## 2.1 Does the weather / seasonal changes have an effect on the total revenue?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "id": "ba7ae1b1",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 576x360 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# get the month of each transaction date and store it in new column\n",
    "hvc_hvvisitresult[\"MONTH\"] = hvc_hvvisitresult[\"DATEONLY\"].apply(lambda x: x.month)\n",
    "\n",
    "# get transactions from 2020 only\n",
    "transactions_2019 = hvc_hvvisitresult[(hvc_hvvisitresult[\"DATEONLY\"] >= pd.Timestamp(year=2019, day=1, month=1)) & (hvc_hvvisitresult[\"DATEONLY\"] < pd.Timestamp(year=2020, month=1, day=1))]\n",
    "\n",
    "# get transactions with succesfull outcome\n",
    "transactions_2019 = transactions_2019[transactions_2019[\"HVOUTCOME_NRID\"] == 2]\n",
    "\n",
    "# get the total sales for each month\n",
    "monthly_sales = transactions_2019.groupby(\"MONTH\")[\"AMOUNT\"].sum().reset_index()\n",
    "\n",
    "# plot barplot\n",
    "\n",
    "# define figure canvas\n",
    "plt.figure(figsize=(8, 5))\n",
    "# plot barplot\n",
    "plt.bar(x=range(12), height=monthly_sales[\"AMOUNT\"])\n",
    "# set xticks as the first three letters of the name of each month\n",
    "month_names = [\"JAN\", \"FEB\", \"MAR\", \"APR\", \"MAY\", \"JUN\", \"JUL\", \"AUG\", \"SEP\", \"OCT\", \"NOV\", \"DEC\"]\n",
    "plt.xticks(range(12), month_names)\n",
    "# define title\n",
    "plt.title(\"Monthly Sales of 2019\")\n",
    "plt.show()"
   ]
  },
